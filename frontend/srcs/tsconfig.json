{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "strict": false,
		"moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "skipLibCheck": true
	},
  "include": ["ts/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}

