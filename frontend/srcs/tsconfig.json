{
	"compilerOptions":
	{
		"target": "ES2017",
		"module": "es2022",
		"lib": ["ES2017", "DOM"],
		"outDir": "dist",
		"rootDir": "ts",
		"strict": false,
		"baseUrl": "./ts",
		"paths": {
			"@core/*": ["core/*"],
			"@modules/*": ["modules/*"]
		}
	},
	"include": [ "ts/**/*.ts" ],
	"exclude": [ "../../node_modules" ]
}
